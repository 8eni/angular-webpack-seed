{
  "name": "angular-webpack-seed",
  "version": "0.1.0",
  "description": "angualr project template",
  "main": "index.js",
  "scripts": {
    "build": "babel-node -- ./node_modules/webpack/bin/webpack.js  --config webpack-production.config.js --progress --profile --colors",
    "build-dev": "babel-node -- ./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress --profile --colors",
    "dev-server": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack-dev-server.config.js --progress --colors --port 2992 --inline --content-base public",
    "test": "babel-node -- ./node_modules/karma-cli/bin/karma start",
    "lint": "eslint ./src/"
  },
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "angular": "^1.4.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.1",
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.2.2",
    "babel-runtime": "^5.6.15",
    "css-loader": "^0.23.0",
    "eslint": "^1.6.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "postcss-loader": "^0.8.0",
    "sass-loader": "^3.0.0",
    "stats-webpack-plugin": "^0.2.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1"
  }
}
